const unsigned char* princesses[] = {anna, ariel, aurora, belle, cinderella, elsa, jasmine, merida, mulan, pocahontas, rapunzel, snowwhite, tiana};

CRGB leds[NUM_LEDS];

short int princessArray[NUM_PRINCESS];

short int pixelArray[NUM_LEDS];

void setup() {
  //Set up display order
  randomSeed(analogRead(0));
  for(int i=0; i < NUM_PRINCESS; i++)
  {
    princessArray[i] = i;
  }

  for(int i=0; i < NUM_LEDS; i++)
  {
    pixelArray[i] = i;
  }

  
  FastLED.addLeds<NEOPIXEL, DATA_PIN>(leds, NUM_LEDS);
  FastLED.setBrightness(25);

  shuffle(princessArray, NUM_PRINCESS);
  shuffle(pixelArray, NUM_LEDS);

}

void loop() 
{
  for(int i=0;i < NUM_PRINCESS; i++)
  {
    for(int j=0; j < NUM_LEDS; j++)
    {
      leds[pixelArray[j]].r = pgm_read_byte_near(princesses[princessArray[i]] + pixelArray[j]*3);
      leds[pixelArray[j]].g = pgm_read_byte_near(princesses[princessArray[i]] + pixelArray[j]*3 +1);
      leds[pixelArray[j]].b = pgm_read_byte_near(princesses[princessArray[i]] + pixelArray[j]*3 +2);
      
      FastLED.show();
      delay(5);
    }
    //FastLED.show();
    delay(180000);
  }
  shuffle(princessArray, NUM_PRINCESS);
  shuffle(pixelArray, NUM_LEDS);
}

void shuffle(short int* arr, short int arrSize)
{
  for(int i=0; i < arrSize; i++)
  {
    int n = random(i, arrSize);
    int tmp = arr[n];
    arr[n] = arr[i];
    arr[i] = tmp;
  }
}

